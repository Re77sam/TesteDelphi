unit ModelConta;

interface

type
  TConta = class
  private
    FCodigoConta: string;
    FNumeroConta: string;
    FDigitoConta: string;
    FCodigoAgencia: string;
    FNumeroAgencia: string;
    FTitularConta: string;
    FGerenteConta: string;
  public
    property CodigoConta: string read FCodigoConta write FCodigoConta;
    property NumeroConta: string read FNumeroConta write FNumeroConta;
    property DigitoConta: string read FDigitoConta write FDigitoConta;
    property CodigoAgencia: string read FCodigoAgencia write FCodigoAgencia;
    property NumeroAgencia: string read FNumeroAgencia write FNumeroAgencia;
    property TitularConta: string read FTitularConta write FTitularConta;
    property GerenteConta: string read FGerenteConta write FGerenteConta;
  end;

implementation

end.

